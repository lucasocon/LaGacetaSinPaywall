{
  "manifest_version": 2,
  "name": "LaGacetaSinPaywall",
  "version": "0.2",
  "description": "An extension to avoid paywall in LaGaceta website",
  "author": "Lucas Ocon",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "persistent": true,
    "scripts": ["js/background.js"]
  }
}
